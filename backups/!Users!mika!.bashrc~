# Setup PYTHONPATH for comtravo
function join { local IFS="$1"; shift; echo "$*"; }

CP1=`ls -d $HOME/comtravo/ct-backend/python_lib/**`
CP1=`join : $CP1`
export CP=$PYTHONPATH:$CP1

