from .spacy import SpacyTransformer  # noqa
from .merge import MergeTransformer  # noqa
from ..base import Transformer


class TestTransformer(Transformer):
    def __init__(self, name, i):
        super(TestTransformer, self).__init__(id=name, input_keys=i, output_key=name)
        self.name = name

    def transform(self, *args):
        return ['{}.transform({})'.format(self.name, ', '.join(map(str, tup)))
                for tup in zip(*args)]
