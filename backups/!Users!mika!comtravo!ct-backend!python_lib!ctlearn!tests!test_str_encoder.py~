from unittest import TestCase
from ctlearn.utils import StrEncoder


class StrEncoderTest(TestCase):
    def test_it(self):
        s = StrEncoder()
        self.assertEqual(s('a'), 1)
        self.assertEqual(s('b'), 2)
        self.assertEqual(s('c'), 3)
        s.freeze()
        self.assertEqual(s('a'), 1)
        self.assertEqual(s('b'), 2)
        self.assertEqual(s('c'), 3)
        self.assertEqual(s('d'), 0)
        
