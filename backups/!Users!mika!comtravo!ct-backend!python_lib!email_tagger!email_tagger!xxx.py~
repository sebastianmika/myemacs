import re
codes = []                                                                                                                                                                                                                                             
rr = re.compile(r'.* LATIN (?:SMALL|CAPITAL) LETTER .*')                                                                                                                                                                                              
with open('../../../data/UnicodeData.txt', 'r') as f:                                                                                                                                                                                                  
    l = f.readline()                                                                                                                                                                                                                                   
    while l:                                                                                                                                                                                                                                           
        if rr.match(l):                                                                                                                                                                                                                                
            codes.append(l.split(';')[1])                                                                                                                                                                                                              
        l = f.readline()                                                                                                                                                                                                                               

                      
