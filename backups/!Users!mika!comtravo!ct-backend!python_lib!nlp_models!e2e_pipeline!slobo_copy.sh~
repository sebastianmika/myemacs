#!/bin/bash
elasticdump \
  --input=$BI_URI/booking_items \
  --output=$BI_URI/booking_items_1 \
  --type=analyzer \
  --awsAccessKeyId=$ACCESS_KEY \
  --awsSecretAccessKey=$SECRET_KEY
elasticdump \
  --input=$BI_URI/booking_items \
  --output=$BI_URI/booking_items_1 \
  --type=mapping \
  --awsAccessKeyId=$ACCESS_KEY \
  --awsSecretAccessKey=$SECRET_KEY
elasticdump \
  --input=$BI_URI/booking_items \
  --output=$BI_URI/booking_items_1 \
  --type=data \
  --searchBody='{"query": { "match_all": {} }, "fields": ["*"], "_source": {"exclude": ["_id"] }}' \
  --awsAccessKeyId=$ACCESS_KEY \
  --awsSecretAccessKey=$SECRET_KEY
