from cachetools.func import ttl_cache
import pandas as pd
from .. gtest import targets


class BaseTarget:
    def __init__(self):
        targets.append(self)

    def search(self):
        return "Constant"

    @ttl_cache(maxsize=32, ttl=60)
    def query(self, from_, to_, freq):
        ts = pd.date_range(from_, to_, freq)
        return [(1, t) for t in ts]
